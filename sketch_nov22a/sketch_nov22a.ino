void setup() {
  // put your setup code here, to run once:


  pinMode (8, OUTPUT);
  pinMode (3, OUTPUT);
  pinMode (4, OUTPUT);
  pinMode (5, OUTPUT);
  pinMode (6, OUTPUT);
  pinMode (7, OUTPUT);


}

void loop() {
  // put your main code here, to run repeatedly:


  digitalWrite(8 ,HIGH);
  delay(1000);
  digitalWrite(3 ,HIGH);
  delay(1000);
  digitalWrite(4 ,HIGH);
  delay(1000);
  digitalWrite(5 ,HIGH);
  delay(1000);
  digitalWrite(6 ,HIGH);
  delay(1000);
  digitalWrite(7 ,HIGH);
  delay(1000);



}
